{
  "name": "server",
  "version": "1.0.0",
  "overrides": {
    "chalk": "4.1.2",
    "strip-ansi": "6.0.1",
    "color-convert": "1.9.3",
    "color-name": "1.1.3",
    "error-ex": "1.3.1",
    "has-ansi": "4.0.1",
    "cliui": "7.0.4",
    "is-core-module": "2.12.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test:ci": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --coverage --setupFilesAfterEnv=./tests/setupTestsIntegration.js",
    "test:unit": "NODE_ENV=test npx jest --selectProjects unit --runInBand --detectOpenHandles --coverage",
    "test:integration": "docker compose -f ../docker-compose.test.yml down -v && docker compose -f ../docker-compose.test.yml up -d && NODE_ENV=test npx jest --selectProjects integration --runInBand --detectOpenHandles --coverage && docker compose -f ../docker-compose.test.yml down -v",
    "test:all": "npm run test:unit && npm run test:integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "axios": "^1.12.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.2.0",
    "helmet": "^8.1.0",
    "html-to-image": "^1.11.13",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "prom-client": "^15.1.3",
    "react-router-dom": "^6.30.1",
    "rotating-file-stream": "^3.2.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.14.1",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  }
}
